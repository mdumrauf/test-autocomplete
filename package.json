{
  "name": "react-autocomplete",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.15",
    "query-string": "^6.9.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-router-dom": "^5.1.2",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "concurrently \"npm run start:server\" \"npm run start:dev-build\"",
    "start:server": "node server/index.js",
    "start:dev-build": "react-scripts start",
    "build": "react-scripts build",
    "test": "concurrently \"npm run test:server\" \"npm run test:app\"",
    "test:app": "react-scripts test --watchAll",
    "test:server": "jest --verbose --color test/server.test.js",
    "test:coverage": "jest --coverage --verbose --color",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:4000",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-react": "^7.17.0",
    "supertest": "^4.0.2"
  }
}
